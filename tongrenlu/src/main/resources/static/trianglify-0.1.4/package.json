{
  "name": "trianglify",
  "version": "0.1.4",
  "description": "Trianglify is a javascript library for generating colorful triangle meshes that can be used as SVG images and CSS backgrounds.",
  "main": "trianglify.js",
  "dependencies": {
    "d3": "3",
    "jsdom": "0.10",
    "xmldom": "0.1",
    "btoa": "1"
  },
  "directories": {
    "example": "examples"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/qrohlf/trianglify.git"
  },
  "keywords": [
    "svg",
    "d3.js",
    "visualization"
  ],
  "author": "Quinn Rohlf <qr@qrohlf.com>",
  "license": "GPLv3",
  "bugs": {
    "url": "https://github.com/qrohlf/trianglify/issues"
  },
  "homepage": "https://github.com/qrohlf/trianglify",
  "devDependencies": {
    "gulp": "^3.8.6",
    "jshint-stylish": "^0.4.0",
    "gulp-uglify": "^0.3.1",
    "gulp-jshint": "^1.7.1",
    "del": "^0.1.1",
    "gulp-rename": "^1.2.0"
  },
  "npmName": "trianglify",
  "npmFileMap": [{
    "basePath": "/",
    "files": [
      "trianglify.js",
      "trianglify.min.js"
    ]
  }]
}
